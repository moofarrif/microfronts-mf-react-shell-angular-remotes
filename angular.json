{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "./apps",
	"projects": {
		"left-sidebar": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				},
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": "./apps/left-sidebar",
			"sourceRoot": "./apps/left-sidebar/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "./dist/left-sidebar",
						"index": "./apps/left-sidebar/src/index.html",
						"main": "./apps/left-sidebar/src/main.ts",
						"polyfills": "./apps/left-sidebar/src/polyfills.ts",
						"tsConfig": "./apps/left-sidebar/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"./apps/left-sidebar/src/favicon.ico",
							"./apps/left-sidebar/src/assets"
						],
						"styles": [
							"./apps/left-sidebar/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "./apps/left-sidebar/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "./apps/left-sidebar/src/environments/environment.ts",
									"with": "./apps/left-sidebar/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "./apps/left-sidebar/webpack.prod.config.js",
							"buildOptimizer": true,
							"extractLicenses": true,
							"optimization": true
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "left-sidebar:build:production",
							"extraWebpackConfig": "./apps/left-sideb/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "left-sidebar:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 3002,
						"publicHost": "http://localhost:3002",
						"extraWebpackConfig": "./apps/left-sidebar/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "left-sidebar:build",
						"extraWebpackConfig": "/apps/left-sidebar/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "/apps/left-sidebar/src/test.ts",
						"polyfills": "/apps/left-sidebar/src/polyfills.ts",
						"tsConfig": "/apps/left-sidebar/tsconfig.spec.json",
						"karmaConfig": "/apps/left-sidebar/karma.conf.js",
						"inlineStyleLanguage": "scss",
						"assets": [
							"/apps/left-sidebar/src/favicon.ico",
							"/apps/left-sidebar/src/assets"
						],
						"styles": [
							"/apps/left-sidebar/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		}
  }
}
